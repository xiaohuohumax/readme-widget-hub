{
  "title": "Pixel GitHub Card Generator",
  "title:pt-BR": "Pixel criação de cartão para GitHub (Pixel GitHub Card Generator)",
  "description": "依据你的 GitHub 数据生成像素艺术卡片。",
  "description:en-US": "Generate pixel art profiles from your GitHub data.",
  "description:pt-BR": "Geração de cartões pixelados com os dados do GitHub.",
  "tag": {
    "repo": "https://github.com/LuciNyan/pixel-profile",
    "onlineTool": "https://pixel-profile-generator.vercel.app/",
    "officialDoc": "https://github.com/LuciNyan/pixel-profile"
  },
  "linkRules": {
    "querys": [
      {
        "name": "username",
        "type": "string",
        "required": true,
        "description": "GitHub 用户名。",
        "description:en-US": "GitHub's username.",
        "description:pt-BR": "Nome do usuário no GitHub.",
        "moreDescription": "例如：`imhalid`。",
        "moreDescription:en-US": "Example: `imhalid`.",
        "moreDescription:pt-BR": "Exemplo: `imhalid`."
      },
      {
        "name": "background",
        "type": "string",
        "default": "#434343",
        "description": "设置背景颜色/图片。支持 CSS 背景属性的子集。",
        "description:en-US": "Set background color/image. Supports a subset of CSS background property values.",
        "description:pt-BR": "Define a cor/imagem de fundo. Suporta um subconjunto de valores de propriedades de fundo CSS."
      },
      {
        "name": "color",
        "type": "string",
        "default": "white",
        "description": "将文本颜色设置为任何有效的 CSS 颜色值。",
        "description:en-US": "Set text color to any valid CSS color value.",
        "description:pt-BR": "Alterar a cor do texto com alguma cor com valor do CSS."
      },
      {
        "name": "hide",
        "type": "string",
        "description": "隐藏特定统计信息或元素。",
        "description:en-US": "Hide specific statistics or elements.",
        "description:pt-BR": "Esconder estatísticas ou elementos específicos.",
        "moreDescription": "可选值：`avatar`, `commits`, `contributions`, `issues`, `prs`, `rank`, `stars`。",
        "moreDescription:en-US": "Optional values: `avatar`, `commits`, `contributions`, `issues`, `prs`, `rank`, `stars`.",
        "moreDescription:pt-BR": "Valores opcionais: `avatar`, `commits`, `contributions`, `issues`, `prs`, `rank`, `stars`."
      },
      {
        "name": "include_all_commits",
        "type": "boolean",
        "default": "false",
        "description": "统计所有提交。",
        "description:en-US": "Count all commits.",
        "description:pt-BR": "Contar todos os commits."
      },
      {
        "name": "pixelate_avatar",
        "type": "boolean",
        "default": "true",
        "description": "对头像应用像素化。",
        "description:en-US": "Apply pixelation to avatar.",
        "description:pt-BR": "Aplicar pixelização no avatar."
      },
      {
        "name": "screen_effect",
        "type": "boolean",
        "default": "false",
        "description": "启用弯曲屏幕效果。",
        "description:en-US": "Enable curved screen effect.",
        "description:pt-BR": "Habilitar efeito de tela curvada."
      },
      {
        "name": "theme",
        "type": "string",
        "description": "主题。",
        "description:en-US": "Theme.",
        "description:pt-BR": "Tema.",
        "moreDescription": "可选值：`journey`, `road_trip`, `fuji`, `summer`, `lax`, `crt`, `monica`。",
        "moreDescription:en-US": "Optional values: `journey`, `road_trip`, `fuji`, `summer`, `lax`, `crt`, `monica`.",
        "moreDescription:pt-BR": "Valores opcionais: `journey`, `road_trip`, `fuji`, `summer`, `lax`, `crt`, `monica`."
      },
      {
        "name": "dithering",
        "type": "boolean",
        "default": "false",
        "description": "使用 256 色调色板渲染图像并应用抖动。",
        "description:en-US": "Render image with 256 color palette and apply dithering.",
        "description:pt-BR": "Renderizar imagem com paleta de 256 cores e aplicar dithering."
      }
    ],
    "uriTemplate": "https://pixel-profile.vercel.app/api/github-stats{?username,background,color,hide,include_all_commits,pixelate_avatar,screen_effect,theme,dithering}"
  },
  "examples": [
    {
      "description": "默认卡片",
      "description:en-US": "Default Card",
      "description:pt-BR": "Cartão padrão",
      "annotate": "imhalid",
      "alt": "Github Card Generator",
      "href": "https://pixel-profile.vercel.app/api/github-stats?username=imhalid&screen_effect=true&dithering=true&include_all_commits=true&pixelate_avatar=true&background=radial-gradient%28circle+at+50%25+50%25%2C+%233ddb82+0%25%2C+%231c1c45+100%25%29&color=%23ffffffFF",
      "images": [
        {
          "alt": "Github Card Generator",
          "href": "https://pixel-profile.vercel.app/api/github-stats?username=imhalid&screen_effect=true&dithering=true&include_all_commits=true&pixelate_avatar=true&background=radial-gradient%28circle+at+50%25+50%25%2C+%233ddb82+0%25%2C+%231c1c45+100%25%29&color=%23ffffffFF"
        }
      ]
    },
    {
      "description": "Fuji 主题并且不像素化头像",
      "description:en-US": "Fuji theme without pixelated avatar",
      "description:pt-BR": "Tema Fuji sem o pixelizado no avatar",
      "annotate": "LuciNyan",
      "alt": "Github Card Generator",
      "href": "https://pixel-profile.vercel.app/api/github-stats?username=LuciNyan&theme=fuji&pixelate_avatar=false",
      "images": [
        {
          "alt": "Github Card Generator",
          "href": "https://pixel-profile.vercel.app/api/github-stats?username=LuciNyan&theme=fuji&pixelate_avatar=false"
        }
      ]
    }
  ]
}