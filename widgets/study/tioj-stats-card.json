  "linkRules": {
    "querys": [
      {
        "name": "username",
        "type": "string",
        "required": true,
        "description": "TIOJ 用户名。",
        "description:en-US": "Username in TIOJ's website.",
        "description:pt-BR": "Nome do usuário no site TIOJ.",
        "moreDescription": "例如：`casperwang`。",
        "moreDescription:en-US": "Example: `casperwang`.",
        "moreDescription:pt-BR": "Example: `casperwang`."
      },
      {
        "name": "style",
        "type": "string",
        "default": "default",
        "description": "卡片样式。",
        "description:en-US": "Card Style.",
        "description:pt-BR": "Estilo do cartão.",
        "moreDescription": "可选值：`default` `dark` `auto`。",
        "moreDescription:en-US": "Supported: `default` `dark` `auto`.",
        "moreDescription:pt-BR": "Suporta: `default` `dark` `auto`."
      },
      {
        "name": "width",
        "type": "number",
        "default": "500",
        "description": "卡片宽度。",
        "description:en-US": "Card Width.",
        "description:pt-BR": "Largura do cartão."
      },
      {
        "name": "height",
        "type": "number",
        "default": "200",
        "description": "卡片高度。",
        "description:en-US": "Card Height.",
        "description:pt-BR": "Altura do cartão."
      },
      {
        "name": "animation",
        "type": "boolean",
        "default": "true",
        "description": "是否启用动画。",
        "description:en-US": "Enable Animation.",
        "description:pt-BR": "Habilitar animação."
      },
      {
        "name": "border",
        "type": "number",
        "default": "1",
        "description": "边框宽度。",
        "description:en-US": "Border Width.",
        "description:pt-BR": "Largura da borda."
      },
      {
        "name": "border_radius",
        "type": "number",
        "default": "4",
        "description": "边框圆角。",
        "description:en-US": "Border Radius.",
        "description:pt-BR": "Raio da borda."
      }
    ],
    "uriTemplate": "https://tioj.card.workers.dev/{?username,style,width,height,animation,border,border_radius}"
  },
