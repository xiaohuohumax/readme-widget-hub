  "linkRules": [
    {
      "params": [
        {
          "name": "pathName",
          "type": "string",
          "description": "路径名",
          "description:en-US": "Path Name",
          "moreDescription": "可选值：`all` `icons` `i`",
          "moreDescription:en-US": "Supported: `all` `icons` `i`"
        }
      ],
      "description": "显示全部图标",
      "description:en-US": "Show All Icons",
      "uriTemplate": "https://mobaicons.com/{pathName}"
    },
    {
      "params": [
        {
          "name": "pathName",
          "type": "string",
          "description": "路径名",
          "description:en-US": "Path Name",
          "moreDescription": "可选值：`icons` `i`",
          "moreDescription:en-US": "Supported: `icons` `i`"
        },
        {
          "name": "names",
          "type": "string",
          "description": "图标名",
          "description:en-US": "Icon Name",
          "moreDescription": "多个图标名用 `,` 分隔, 例如：`html,css,ts,hono`",
          "moreDescription:en-US": "Multiple names are separated by `,`, e.g. `html,css,ts,hono`"
        }
      ],
      "querys": [
        {
          "name": "sort",
          "type": "boolean",
          "default": "false",
          "description": "是否按字母顺序排序",
          "description:en-US": "Sort by Alphabetical Order"
        },
        {
          "name": "duplicate",
          "type": "boolean",
          "default": "false",
          "description": "是否显示重复的图标",
          "description:en-US": "Show Duplicated Icons"
        },
        {
          "name": "perline",
          "type": "number",
          "default": "15",
          "description": "每行显示的图标数量",
          "description:en-US": "Number of Icons per Line"
        }
      ],
      "description": "显示指定图标",
      "description:en-US": "Show Specified Icons",
      "uriTemplate": "https://mobaicons.com/{pathName}/{names}{?sort,duplicate,perline}"
    },
    {
      "params": [
        {
          "name": "value",
          "type": "string",
          "description": "搜索值",
          "description:en-US": "Search Value"
        }
      ],
      "querys": [
        {
          "name": "perline",
          "type": "number",
          "default": "15",
          "description": "每行显示的图标数量",
          "description:en-US": "Number of Icons per Line"
        }
      ],
      "description": "搜索图标",
      "description:en-US": "Search Icons",
      "uriTemplate": "https://mobaicons.com/search/{value}{?perline}"
    }
  ],
